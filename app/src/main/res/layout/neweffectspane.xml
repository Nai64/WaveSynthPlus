<?xml version="1.0" encoding="utf-8"?>
<ScrollView
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:fillViewport="true">
<LinearLayout
	android:id="@+id/newEffectsPane"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">
	<TableLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:shrinkColumns="0"
		android:stretchColumns="1">
		<TableRow
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1">
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="5dp"
				android:text="Reverb"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textSize="14sp"
				android:shadowColor="#80000000"
				android:shadowDx="2"
				android:shadowDy="2"
				android:shadowRadius="1"/>
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="Amount "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/reverbAmount"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Room Size "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/reverbRoomSize"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Damping "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/reverbDamping"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
			</LinearLayout>
		</TableRow>
		<TableRow
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1">
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="5dp"
				android:text="Limiter"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textSize="14sp"
				android:shadowColor="#80000000"
				android:shadowDx="2"
				android:shadowDy="2"
				android:shadowRadius="1"/>
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="Amount "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/limiterAmount"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Threshold "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/limiterThreshold"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Release "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/limiterRelease"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
			</LinearLayout>
		</TableRow>
		<TableRow
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1">
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="5dp"
				android:text="Compressor"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:textSize="14sp"
				android:shadowColor="#80000000"
				android:shadowDx="2"
				android:shadowDy="2"
				android:shadowRadius="1"/>
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="Amount "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/compressorAmount"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Ratio "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/compressorRatio"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="5dp"
					android:text="  Attack "
					android:textSize="13sp"
					android:shadowColor="#80000000"
					android:shadowDx="2"
					android:shadowDy="2"
					android:shadowRadius="1"/>
				<com.gallantrealm.android.Slider
					android:id="@+id/compressorAttack"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_weight="1"
					android:max="100"
					android:progressDrawable="@drawable/progress_horizontal"
					android:thumb="@drawable/seek_thumb"/>
			</LinearLayout>
		</TableRow>
	</TableLayout>
</LinearLayout>
</ScrollView>
